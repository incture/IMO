<mvc:View controllerName="com.sap.incture.IMO_PM.controller.WOList" xmlns:mvc="sap.ui.core.mvc" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns="sap.m" xmlns:fb="sap.ui.comp.filterbar" height="100%" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:table="sap.ui.table"
	xmlns:tnt="sap.tnt">
	<Page showHeader="false" class="sapUiSizeCompact sapMPageBackground" enableScrolling="true">
		<Toolbar class="headerBG headerShadow" height="2.75rem">
			<Label design="Bold" class="sapUiSmallMarginBegin textColr" text="{i18n>tile_woList}" wrapping="true"/>
			<ToolbarSpacer/>
			<ComboBox showSecondaryValues="true" items="{path:'mLookupModel>/aWorkCenterSet',sorter:{ path: 'text' }}"
				selectedKey="{mLookupModel>/sWorkCenterSel}" placeholder="{i18n>WorkCenter_KPI}" class="inputRadius sapUiTinyMarginEnd"
				tooltip="{i18n>SELECT_WORK_CENTER}" change="fnFetchKPIInfo">
				<core:ListItem key="{mLookupModel>WorkcenterId}" additionalText="{mLookupModel>WorkcenterDesc}" text="{mLookupModel>WorkcenterId}"/>
			</ComboBox>
		</Toolbar>
		<content>
			<l:Grid defaultSpan="XL3 L3 M6 S12" class="sapUiTinyMarginBeginEnd sapUiTinyMarginBottom sapUiSmallMarginTop">
				<GenericTile press="getKPIsWoList" class="murphyHdrTileClass KPIShadow" ariaLabel="WO_LIST_PM_WORK_DUE">
					<TileContent>
						<HBox alignItems="Center" class="sapUiSmallMargin">
							<FlexBox class="murphyAmberIconFlex" alignItems="Center" justifyContent="Center">
								<items>
									<core:Icon src="sap-icon://pending" color="#ffffff" size="20px"/>
								</items>
							</FlexBox>
							<HBox width="100%" class="sapUiTinyMarginBegin" justifyContent="SpaceBetween" alignItems="Center">
								<Text class="notificationHeaderText" wrapping="true" text="PM&#10;Work Order Due"/>
								<Text text="{mLookupModel>/oKPIInfo/WO_due}" class="formatKPINumber"/>
							</HBox>
						</HBox>
					</TileContent>
				</GenericTile>
				<GenericTile press="getKPIsWoList" class="murphyHdrTileClass KPIShadow" ariaLabel="WO_LIST_ASSIGEND_TO_ME">
					<TileContent>
						<HBox alignItems="Center" class="sapUiSmallMargin">
							<FlexBox class="murphyBlueIconFlex" alignItems="Center" justifyContent="Center">
								<items>
									<core:Icon src="sap-icon://activity-2" color="#ffffff" size="20px"/>
								</items>
							</FlexBox>
							<HBox width="100%" class="sapUiTinyMarginBegin" justifyContent="SpaceBetween" alignItems="Center">
								<Text class="notificationHeaderText" wrapping="true" text="{i18n>asigned}"/>
								<Text text="{mLookupModel>/oKPIInfo/Assign_to_me}" class="formatKPINumber"/>
							</HBox>
						</HBox>
					</TileContent>
				</GenericTile>
				<GenericTile press="getKPIsWoList" class="murphyHdrTileClass KPIShadow" ariaLabel="WO_LIST_SPARE_PART_RESERV">
					<TileContent>
						<HBox alignItems="Center" class="sapUiSmallMargin">
							<FlexBox class="murphyDeepBlueIconFlex" alignItems="Center" justifyContent="Center">
								<items>
									<core:Icon src="sap-icon://settings" color="#ffffff" size="20px"/>
								</items>
							</FlexBox>
							<HBox width="100%" class="sapUiTinyMarginBegin" justifyContent="SpaceBetween" alignItems="Center">
								<Text class="notificationHeaderText" wrapping="true" text="Outstanding Spare&#10;Part Reservations"/>
								<Text text="{mLookupModel>/oKPIInfo/Otstndng_spare_prt}" class="formatKPINumber"/>
							</HBox>
						</HBox>
					</TileContent>
				</GenericTile>
				<GenericTile press="getKPIsWoList" class="murphyHdrTileClass KPIShadow" ariaLabel="WO_LIST_ASSIGNED_TO_WC">
					<TileContent>
						<HBox alignItems="Center" class="sapUiSmallMargin">
							<FlexBox class="murphyBlueIconFlex" alignItems="Center" justifyContent="Center">
								<items>
									<core:Icon src="sap-icon://activity-2" color="#ffffff" size="20px"/>
								</items>
							</FlexBox>
							<HBox width="100%" class="sapUiTinyMarginBegin" justifyContent="SpaceBetween" alignItems="Center">
								<Text class="notificationHeaderText" wrapping="true" text="Assigned to&#10;Work Center"/>
								<Text text="{mLookupModel>/oKPIInfo/Assign_to_WC}" class="formatKPINumber"/>
							</HBox>
						</HBox>
					</TileContent>
				</GenericTile>
				<!--<ToolbarSpacer/>
				<ComboBox class="inputRadius" width="90%" showSecondaryValues="true" items="{path:'mLookupModel>/aWorkCenterSet',sorter:{ path: 'text' }}"
					selectedKey="{mLookupModel>/sWorkCenterSel}" placeholder="{i18n>WorkCenter_KPI}" tooltip="{i18n>SELECT_WORK_CENTER}" change="fnFetchKPIInfo">
					<core:ListItem key="{mLookupModel>WorkcenterId}" additionalText="{mLookupModel>WorkcenterDesc}" text="{mLookupModel>WorkcenterId}"/>
				</ComboBox>-->
			</l:Grid>
