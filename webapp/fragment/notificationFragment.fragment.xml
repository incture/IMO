				<Panel expandable="true" class="clIopNotifPanel" busy="{dashBoardModel>/webSocketProperty/alarmsInd}" busyIndicatorDelay="0" width="auto"
					visible='{parts:["dashBoardModel>/userData","dashBoardModel>/webSocketProperty/data/alarmNotificationDto"], formatter:"com.sap.incture.Incture_IOP.util.formatter.fnNotifListItem"}'>
					<headerToolbar>
						<Toolbar height="3rem">
							<Link text="Alarms ( {dashBoardModel>/webSocketProperty/data/alarmNotificationDto/alarmCount} )" class="clIopNotifLink"></Link>
							<ToolbarSpacer></ToolbarSpacer>
							<Button press="onAcknowledgeAlarmsNotif" text="Clear All" tooltip="Clear All" type="Accept"
								class="clIopNotifClearAllButton sapUiSmallMarginEnd"></Button>
						</Toolbar>
					</headerToolbar>
					<content>
						<List items="{dashBoardModel>/webSocketProperty/data/alarmNotificationDto/alarmNotificationList}" growing="true" growingThreshold="25"
							class="clIopNotifList">
							<CustomListItem highlight="Information" type="Active" press="onAlarmListItemPress">
								<VBox justifyContent="SpaceBetween" class="iopTaskManagementVBoxStyle">
									<HBox justifyContent="SpaceBetween">
										<Title wrapping="true" class="clIopNotifPanelSubject"
											text='{parts:["dashBoardModel>facilityDesc","dashBoardModel>locationText"], formatter:"com.sap.incture.Incture_IOP.util.formatter.fnAlarmsNotifTitle"}'></Title>
										<Button icon="sap-icon://decline" type="Transparent" tooltip="Clear" press="onAlarmAlertClose"></Button>
									</HBox>
									<HBox justifyContent="SpaceBetween">
										<Label wrapping="true" class="clIopNotifPanelSecondItem"
											text='{parts:["dashBoardModel>facilityDesc","dashBoardModel>longDesc"], formatter:"com.sap.incture.Incture_IOP.util.formatter.fnAlarmsNotifDesc"}'></Label>
									</HBox>
									<HBox class="clIopNotifPanelLastItem">
										<Text text="Well Status: {dashBoardModel>status}"></Text>
									</HBox>
								</VBox>
							</CustomListItem>
						</List>
					</content>
				</Panel>
				<Panel expandable="true" class="clIopNotifPanel" busy="{dashBoardModel>/webSocketProperty/fracInd}" busyIndicatorDelay="0" width="auto"
					visible='{parts:["dashBoardModel>/userData","dashBoardModel>/webSocketProperty/data/fracNotificationDto"], formatter:"com.sap.incture.Incture_IOP.util.formatter.fnNotifListItem"}'>
					<headerToolbar>
						<Toolbar height="3rem">
							<Link text="Frac Monitoring ( {dashBoardModel>/webSocketProperty/data/fracNotificationDto/fracCount} )" class="clIopNotifLink"></Link>
							<ToolbarSpacer></ToolbarSpacer>
							<Button press="onAcknowledgeFracNotif" text="Clear All" tooltip="Clear All" type="Accept"
								class="clIopNotifClearAllButton sapUiSmallMarginEnd"></Button>
						</Toolbar>
					</headerToolbar>
					<content>
						<List items="{dashBoardModel>/webSocketProperty/data/fracNotificationDto/fracNotificationList}" growing="true" growingThreshold="25"
							class="clIopNotifList">
							<CustomListItem highlight="Information" type="Active" press="onFracListItemPress">
								<VBox justifyContent="SpaceBetween" class="iopTaskManagementVBoxStyle">
									<HBox justifyContent="SpaceBetween">
										<Title text="Frac ID: {dashBoardModel>fracId} : {dashBoardModel>wellName}" wrapping="true" class="clIopNotifPanelSubject"></Title>
										<Button icon="sap-icon://decline" type="Transparent" tooltip="Clear" press="onFracAlertClose"></Button>
									</HBox>
									<HBox justifyContent="SpaceBetween">
										<Label text="Active Case Pressure: {dashBoardModel>activeCasePressure} , Active Tube Pressure: {dashBoardModel>activeTubePressure}"
											wrapping="true" class="clIopNotifPanelSecondItem"></Label>
									</HBox>
								</VBox>
							</CustomListItem>
						</List>
					</content>
				</Panel>
				<Panel expandable="true" class="clIopNotifPanel" busy="{dashBoardModel>/webSocketProperty/pwHopperInd}" busyIndicatorDelay="0" width="auto"
					visible='{parts:["dashBoardModel>/userData","dashBoardModel>/webSocketProperty/data/pwHopperNotificationDto"], formatter:"com.sap.incture.Incture_IOP.util.formatter.fnNotifListItem"}'>
