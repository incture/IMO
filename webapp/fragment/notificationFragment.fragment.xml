						<Toolbar height="3rem">
							<Link text="PW Hopper ( {dashBoardModel>/webSocketProperty/data/pwHopperNotificationDto/hopperCount} )" class="clIopNotifLink"></Link>
							<ToolbarSpacer></ToolbarSpacer>
							<Button press="onAcknowledgePwHopperNotif" text="Clear All" tooltip="Clear All" type="Accept"
								class="clIopNotifClearAllButton sapUiSmallMarginEnd"></Button>
						</Toolbar>
					</headerToolbar>
					<content>
						<List items="{dashBoardModel>/webSocketProperty/data/pwHopperNotificationDto/pwHopperNotificationList}" growing="true" growingThreshold="25"
							class="clIopNotifList">
							<CustomListItem highlight="Information" type="Active" press="onPwHopperListItemPress">
								<VBox justifyContent="SpaceBetween" class="iopTaskManagementVBoxStyle">
									<HBox justifyContent="SpaceBetween">
										<Title text="Location: {dashBoardModel>locationText}" wrapping="true" class="clIopNotifPanelSubject"></Title>
										<Button icon="sap-icon://decline" type="Transparent" tooltip="Clear" press="onPwHopperAlertClose"></Button>
									</HBox>
								</VBox>
							</CustomListItem>
						</List>
					</content>
				</Panel>
				<Panel expandable="true" class="clIopNotifPanel" width="auto" busy="{dashBoardModel>/webSocketProperty/tasksInd}" busyIndicatorDelay="0"
					visible='{parts:["dashBoardModel>/userData","dashBoardModel>/webSocketProperty/data/taskNotificationDto"], formatter:"com.sap.incture.Incture_IOP.util.formatter.fnNotifListItem"}'>
					<headerToolbar>
						<Toolbar height="3rem">
							<Link text="Tasks ( {dashBoardModel>/webSocketProperty/data/taskNotificationDto/taskCount} )" class="clIopNotifLink"></Link>
							<ToolbarSpacer></ToolbarSpacer>
							<Button press="onAcknowledgeTasksNotif" text="Clear All" tooltip="Clear All" type="Accept"
								class="clIopNotifClearAllButton sapUiSmallMarginEnd"></Button>
						</Toolbar>
					</headerToolbar>
					<content>
						<List items="{dashBoardModel>/webSocketProperty/data/taskNotificationDto/taskNotificationList}" growing="true" growingThreshold="25"
							class="clIopNotifList">
							<CustomListItem highlight="Information" type="Active" press="onTaskListItemPress">
								<VBox justifyContent="SpaceBetween" class="iopTaskManagementVBoxStyle">
									<HBox justifyContent="SpaceBetween">
										<Title text="{dashBoardModel>subject}" wrapping="true" class="clIopNotifPanelSubject"></Title>
										<Button icon="sap-icon://decline" type="Transparent" tooltip="Clear" press="onTaskAlertClose"></Button>
									</HBox>
									<HBox justifyContent="SpaceBetween">
										<Label text="Type: {dashBoardModel>module} , Assigned To: {dashBoardModel>assignedTo}" wrapping="true" class="clIopNotifPanelSecondItem"></Label>
									</HBox>
									<HBox class="clIopNotifPanelLastItem">
										<Text textAlign="Center" text="{dashBoardModel>status}" class=" iopwbOriginClass iopTaskManagementTextFont"
											visible='{path:"dashBoardModel>status", formatter:"com.sap.incture.Incture_IOP.util.formatter.fnGetNotifStatusColor"}'/>
									</HBox>
								</VBox>
							</CustomListItem>
						</List>
					</content>
				</Panel>
				<Panel expandable="true" expand="onByPassPanelExpand" class="clIopNotifPanel" busy="{dashBoardModel>/webSocketProperty/bypassLogInd}"
					busyIndicatorDelay="0" width="auto"
					visible='{parts:["dashBoardModel>/userData","dashBoardModel>/webSocketProperty/data/byPassLogNotificationDto"], formatter:"com.sap.incture.Incture_IOP.util.formatter.fnNotifListItem"}'>
					<headerToolbar>
						<Toolbar height="3rem">
							<Link text="Active Bypass Logs ( {dashBoardModel>/webSocketProperty/data/byPassLogNotificationDto/byPassLogCount} )" class="clIopNotifLink"></Link>
							<ToolbarSpacer></ToolbarSpacer>
							<Button press="onAcknowledgeBypassLogNotif" text="Clear All" tooltip="Clear All" type="Accept"
								class="clIopNotifClearAllButton sapUiSmallMarginEnd"></Button>
						</Toolbar>
					</headerToolbar>
					<content>
						<List items="{dashBoardModel>/webSocketProperty/data/byPassLogNotificationDto/byPassLogNotificationList}" growing="true"
							growingThreshold="25" class="clIopNotifList">
							<CustomListItem highlight="Information" type="Active" press="onBypassLogListItemPress">
								<VBox justifyContent="SpaceBetween" class="iopTaskManagementVBoxStyle">
									<HBox justifyContent="SpaceBetween">
										<Title text="Bypass No: {dashBoardModel>bypassNum} : {dashBoardModel>locationText}" wrapping="true" class="clIopNotifPanelSubject"></Title>
										<Button icon="sap-icon://decline" type="Transparent" tooltip="Clear" press="onBypassLogAlertClose"></Button>
									</HBox>
									<HBox justifyContent="SpaceBetween">
										<Label text="{dashBoardModel>safetyAppMsg}" wrapping="true" class="clIopNotifPanelSecondItem"></Label>
									</HBox>
									<HBox class="clIopNotifPanelLastItem">
										<Text text="Severity: {dashBoardModel>severity}"></Text>
									</HBox>
								</VBox>
							</CustomListItem>
						</List>
					</content>
				</Panel>
				<Panel expandable="true" class="clIopNotifPanel" busy="{dashBoardModel>/webSocketProperty/energyIsoInd}" busyIndicatorDelay="0" width="auto"
					visible='{parts:["dashBoardModel>/userData","dashBoardModel>/webSocketProperty/data/energyIsolationNotificationDto"], formatter:"com.sap.incture.Incture_IOP.util.formatter.fnNotifListItem"}'>
					<headerToolbar>
						<Toolbar height="3rem">
							<Link text="Energy Isolation ( {dashBoardModel>/webSocketProperty/data/energyIsolationNotificationDto/energyIsolationCount} )"
								class="clIopNotifLink"></Link>
							<ToolbarSpacer></ToolbarSpacer>
							<Button press="onAcknowledgeEnergyIsoNotif" text="Clear All" tooltip="Clear All" type="Accept"
								class="clIopNotifClearAllButton sapUiSmallMarginEnd"></Button>
						</Toolbar>
					</headerToolbar>
					<content>
						<List items="{dashBoardModel>/webSocketProperty/data/energyIsolationNotificationDto/energyIsolationNotificationList}" growing="true"
							growingThreshold="25" class="clIopNotifList">
							<CustomListItem highlight="Information" type="Active" press="onEnergyIsoListItemPress">
								<VBox justifyContent="SpaceBetween" class="iopTaskManagementVBoxStyle">
									<HBox justifyContent="SpaceBetween">
										<Title text="Energy Isolation No: {dashBoardModel>bypassNum} : {dashBoardModel>locationText}" wrapping="true" class="clIopNotifPanelSubject"></Title>
										<Button icon="sap-icon://decline" type="Transparent" tooltip="Clear" press="onEnergyIsoAlertClose"></Button>
									</HBox>
									<HBox justifyContent="SpaceBetween">
										<Label text="{dashBoardModel>safetyAppMsg}" wrapping="true" class="clIopNotifPanelSecondItem"></Label>
									</HBox>
								</VBox>
							</CustomListItem>
						</List>
					</content>
				</Panel>
			</l:content>
		</l:VerticalLayout>
	</ScrollContainer>
</core:FragmentDefinition>