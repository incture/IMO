<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Dialog title="What's your seat today?" verticalScrolling="false" contentWidth="500px">
		<content>
			<VBox>
				<MultiComboBox id="idChangeSeatMultiComboBox" selectedKeys="{dashBoardModel>/changeSeatData/changeSeatIOPUserRoleKeys}"
					class="sapUiSizeCompact sapUiSmallMarginTop sapUiTinyMarginBeginEnd" items="{dashBoardModel>/changeSeatData/changeSeatIOPUserRoles}"
					valueState="{dashBoardModel>/changeSeatData/changeSeatIOPUserRolesValueState}" width="95%"
					valueStateText="{dashBoardModel>/changeSeatData/changeSeatIOPUserRolesValueStateText}">
					<items>
						<core:Item key="{dashBoardModel>keyIopUserRole}" text="{dashBoardModel>valueIopUserRole}"/>
					</items>
				</MultiComboBox>
				<CheckBox text="Show tasks created by me" useEntireWidth="true" selected="{!dashBoardModel>changeSeatData/changeSeatIOPUserRoleCreatedByMe}"
					select="onChangeSeatCreatedByMeCheckboxSelect" class="sapUiSmallMarginBottom sapUiTinyMarginEnd"/>
			</VBox>
		</content>
		<buttons>
			<Button text="Proceed" press="onChangeSeatProceedButtonPress" class="sapUiSizeCompact iopBtnClass iopwbFooterBtnClass"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>