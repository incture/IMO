<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:layout="sap.ui.layout"
	xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" class="sapUiSizeCompact">
	<Dialog title="Change Corrective Maintenance Order" contentWidth="40%" class="sapUiSizeCompact dialogHeader">
		<!--<Page title="Change Corrective Maintenance Order" showHeader="true" class="whiteBG">-->
		<content>
			<l:Grid class="sapUiSmallMargin">
				<VBox class="sapUiSmallMarginTop">
					<Label text="{i18n>MAT_DESC}"></Label>
					<Input class="inputRadius" value="{oWorkOrderDetailViewModel>/oComponentDetails/MatlDesc}"></Input>
					<layoutData>
						<l:GridData span="XL6 L12 M12 S6"/>
					</layoutData>
				</VBox>
				<VBox class="sapUiSmallMarginTop">
					<Label text="{i18n>MAT_REQ_QTY}"></Label>
					<HBox>
						<Input class="inputRadius" value="{oWorkOrderDetailViewModel>/oComponentDetails/RequirementQuantity}" width="100%"></Input>
						<Input class="inputRadius sapUiTinyMarginBegin" value="{oWorkOrderDetailViewModel>/oComponentDetails/RequirementQuantityUnit}" width="100%"></Input>
					</HBox>
					<layoutData>
						<l:GridData span="XL6 L6 M6 S6"/>
					</layoutData>
				</VBox>
				<VBox class="sapUiSmallMarginTop">
					<Label text="Per"></Label>
					<Input class="inputRadius" value="{oWorkOrderDetailViewModel>/oComponentDetails/PriceUnit}"></Input>
					<layoutData>
						<l:GridData span="XL6 L6 M6 S6"/>
					</layoutData>
				</VBox>
				<VBox class="sapUiSmallMarginTop">
					<Label text="Price"></Label>
					<HBox>
						<Input class="inputRadius" value="{oWorkOrderDetailViewModel>/oComponentDetails/Price}" width="100%"></Input>
						<Input class="inputRadius sapUiTinyMarginBegin" value="{oWorkOrderDetailViewModel>/oComponentDetails/Currency}" width="100%"></Input>
					</HBox>
					<layoutData>
						<l:GridData span="XL6 L6 M6 S6"/>
					</layoutData>
				</VBox>
				<VBox class="sapUiSmallMarginTop">
					<Label text="Material Group"></Label>
					<Input class="inputRadius" value="{oWorkOrderDetailViewModel>/oComponentDetails/MaterialGroup}"></Input>
					<layoutData>
						<l:GridData span="XL6 L6 M6 S6"/>
					</layoutData>
				</VBox>
				<VBox class="sapUiSmallMarginTop">
					<Label text="{i18n>PR_COST_ELEMENT}"></Label>
					<Input value="{oWorkOrderDetailViewModel>/oComponentDetails/GlAccount}"></Input>
					<layoutData>
						<l:GridData span="XL6 L6 M12 S12"/>
					</layoutData>
				</VBox>
				<VBox class="sapUiSmallMarginTop">
					<Label text="Purch. Group"></Label>
					<HBox>
						<Input class="inputRadius" value="{oWorkOrderDetailViewModel>/oComponentDetails/PurchGrp}" width="100%"></Input>
						<Input class="inputRadius sapUiTinyMarginBegin" value="{oWorkOrderDetailViewModel>/oComponentDetails/PurchOrg}" width="100%"></Input>
					</HBox>
					<layoutData>
						<l:GridData span="XL6 L6 M6 S6"/>
					</layoutData>
				</VBox>
				<VBox class="sapUiSmallMarginTop">
					<Label text="Vendor"></Label>
					<Input class="inputRadius" value="{oWorkOrderDetailViewModel>/oComponentDetails/VendorNo}"></Input>
					<layoutData>
						<l:GridData span="XL6 L6 M6 S6"/>
					</layoutData>
				</VBox>
				<VBox class="sapUiSmallMarginTop">
					<Label text="Agreement"></Label>
					<HBox>
						<Input class="inputRadius" value="{oWorkOrderDetailViewModel>/oComponentDetails/Agreement}" width="100%"></Input>
						<Input class="inputRadius sapUiTinyMarginBegin" value="{oWorkOrderDetailViewModel>/oComponentDetails/AgmntItem}" width="100%"></Input>
					</HBox>
					<layoutData>
						<l:GridData span="XL6 L6 M6 S6"/>
					</layoutData>
				</VBox>
				<VBox class="sapUiSmallMarginTop">
					<Label text="Recipient"></Label>
					<Input class="inputRadius" value="{oWorkOrderDetailViewModel>/oComponentDetails/Recipient}" width="100%"></Input>
					<layoutData>
						<l:GridData span="XL6 L6 M6 S6"/>
					</layoutData>
				</VBox>
				<VBox>
					<layoutData>
						<l:GridData span="XL6 L6 M6 S6"/>
					</layoutData>
				</VBox>
				<VBox class="sapUiSmallMarginTop">
					<Label text="Requisitioner"></Label>
					<Input class="inputRadius" value="{oWorkOrderDetailViewModel>/oComponentDetails/Requisitioner}" width="100%"></Input>
					<layoutData>
						<l:GridData span="XL6 L6 M6 S6"/>
					</layoutData>
				</VBox>
			</l:Grid>
		</content>
		<!--<footer>-->
		<!--<Toolbar>-->
		<!--<ToolbarSpacer/>-->
		<buttons>
			<Button class="transparentBtn createNotifClearBtn" icon="sap-icon://decline" iconFirst="true" text="Cancel" press="onCancelItemCatPR"/>
			<Button type="Emphasized" text="Save" press="onSaveItemCatPR">
				<customData>
					<core:CustomData key="dateType" value="CREATE_ORDER"/>
				</customData>
			</Button>
		</buttons>
		<!--</Toolbar>-->
		<!--</footer>-->
		<!--</Page>-->
	</Dialog>
</core:FragmentDefinition >