	<l:Grid defaultSpan="XL2 L2 M2 S12">
		<VBox class="sapUiTinyMargin">
			<Label text="Assigned Tech" class="sapUiTinyMarginTop"/>
			<Input class="inputRadius" width="100%" value="{mLookupModel>/sAssignTechFilter}"></Input>
		</VBox>
		<!--	<Label text="Equipment" class="sapUiTinyMarginTop"/>
				<Input showValueHelp="true" value="{mLookupModel>/sEquipFilter}" ></Input>-->
