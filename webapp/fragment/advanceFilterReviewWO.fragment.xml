			<Input class="inputRadius" width="100%" value="{mLookupModel>/sWOTypeFilter}"></Input>
		</VBox>
		<VBox class="sapUiTinyMargin">
			<Label text="Work Center" class="sapUiTinyMarginTop"/>
			<Input class="inputRadius" width="100%" value="{mLookupModel>/sWorkCenterFilter}"></Input>
		</VBox>
		<Text/>
		<HBox class="sapUiMediumMarginTop">
			<Button width="4rem" text="Clear" press="onCancelDialogWO" type="Transparent" class="sapUiLargeMarginBegin"/>
			<Button width="4rem" text="Apply" press="onApplyFilter" type="Emphasized"/>
		</HBox>
	</l:Grid>
