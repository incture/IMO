							enabled="{oWODetailFieldsModel>/assignedTo}" change="onUpdateAssignedTo" valueHelpOnly="false" class="inputRadius">
							<customData>
								<core:CustomData value="/HEADERTOPARTNERNAV/0/AssignedTo"/>
							</customData>
						</Input>
						<layoutData>
							<l:GridData span="XL2 L2 M4 S6"/>
						</layoutData>
					</VBox>
					<VBox class="sapUiTinyMarginTop">
						<Label text="{i18n>PERSON_RESP_NAME}"></Label>
						<!--	<Input value="{path:'oWorkOrderDetailViewModel>/HEADERTOPARTNERNAV/0/AssignedTo',formatter:'com.mylan.createWorkOrder.formatter.formatter.fnGetNamefromUserid'}" 
							enabled="false" class="inputRadius">
						</Input>-->
						<!--nischal Commented this code...binding is changed from mLookupModel to oWorkOrderDetailViewModel-->
						<!--<Input value="{mLookupModel>/AssigedTechName}" enabled="false" class="inputRadius"></Input>-->
						<Input value = "{oWorkOrderDetailViewModel>/ReportedByName}" enabled = "false" class = "inputRadius"/>
						<layoutData>
							<l:GridData span="XL2 L2 M4 S6"/>
						</layoutData>
					</VBox>
					<VBox class="sapUiTinyMarginTop">
						<HBox>
							<Label text="{i18n>SUPERIOR_WO}"></Label>
							<Link text="View WO" press="onViewSuperiorWO" class="sapUiSmallMarginBegin"
								enabled="{path:'oWorkOrderDetailModel>/SuperOrder', formatter:'com.sap.incture.IMO_PM.formatter.formatter.setSuperiorWOEnabled'}"/>
						</HBox>
						<Input value="{oWorkOrderDetailModel>/SuperOrder}" change="validateIntegerValues" class="inputRadius"
							enabled="{oWODetailFieldsModel>/priority}" width="100%">
							<customData>
								<core:CustomData value="INTEGER"/>
							</customData>
						</Input>
						<!--<HBox fitContainer="true" justifyContent="Start" alignContent="Start">-->
						<!--	<Input value="{oWorkOrderDetailModel>/SuperOrder}" change="validateIntegerValues" class="inputRadius"-->
						<!--		enabled="{oWODetailFieldsModel>/priority}" width="100%">-->
						<!--		<customData>-->
						<!--			<core:CustomData value="INTEGER"/>-->
						<!--		</customData>-->
						<!--	</Input>-->
						<!--<Button text="View WO" press="onViewSuperiorWO" class="sapUiTinyMarginBegin"-->
						<!--	enabled="{path:'oWorkOrderDetailModel>/SuperOrder', formatter:'com.sap.incture.IMO_PM.formatter.formatter.setSuperiorWOEnabled'}">-->
						<!--	<customData>-->
						<!--		<core:CustomData value="{oWorkOrderDetailModel>/SuperOrder}"/>-->
						<!--	</customData>-->
						<!--</Button>-->
						<!--</HBox>-->
						<layoutData>
							<l:GridData span="XL2 L2 M4 S6"/>
						</layoutData>
					</VBox>
					<VBox class="sapUiTinyMarginTop" visible="{oWorkOrderDetailViewModel>/withNotificationCheck}">
						<Label text="{i18n>BREAKDOWN}"></Label>
						<Switch state="{path:'oWorkOrderDetailModel>/Breakdown', formatter:'com.sap.incture.IMO_PM.formatter.formatter.formatBooleanBreakDown'}"
							enabled="{oWODetailFieldsModel>/breakdown}" change="fnUpdateBreakDownStatus"/>
						<layoutData>
							<l:GridData span="XL2 L2 M4 S6"/>
						</layoutData>
					</VBox>
					<VBox class="sapUiTinyMarginTop"
						visible="{parts:['oWorkOrderDetailModel>/Breakdown', 'oWorkOrderDetailViewModel>/withNotificationCheck'], formatter:'com.sap.incture.IMO_PM.formatter.formatter.fnNotifRelatedFieldsVisible'}">
						<Label text="{i18n>BREAKDOWN_DURATION}"></Label>
						<Input value="{oWorkOrderDetailModel>/Downtime}" change="validateBreakDownValue" class="inputRadius"
							enabled="{parts:['oWorkOrderDetailModel>/Breakdown', 'oWorkOrderDetailModel>/OrderStatus'], formatter:'com.sap.incture.IMO_PM.formatter.formatter.enableBreakDownFields'}">
							<customData>
								<core:CustomData value="FLOAT"/>
							</customData>
						</Input>
						<layoutData>
							<l:GridData span="XL2 L2 M4 S6"/>
						</layoutData>
					</VBox>
					<!--<VBox class="sapUiTinyMarginTop">-->
					<!--	<Label text="{i18n>DAMAGE_CODE}"></Label>-->
