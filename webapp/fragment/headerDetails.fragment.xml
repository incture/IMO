	<Panel expanded="{oWorkOrderDetailViewModel>/notificationDetails}" expandable="true" class="sapMPanelWOHeader panelBackgroundClass"
		expand="onOpenHideHeaderSections">
		<customData>
			<core:CustomData key="panelType" value="NOTIFICATIONS_DETAILS"/>
		</customData>
		<headerToolbar>
			<OverflowToolbar>
				<Text text="{path:'oWorkOrderDetailModel>/HEADERTONOTIFNAV', formatter:'.formatter.formatNotificationCount'}"
					class="leftMargin panelTextColor"></Text>
				<ToolbarSpacer/>
				<Button text="{i18n>DETAIL_ADD_NOTIFICATION}" icon="sap-icon://add" iconFirst="true" class="sapUiSmallMarginBeginEnd"
					press="onOpenNotificationPopup" type="Transparent"
					visible="{path:'oWorkOrderDetailModel>/OrderStatus', formatter:'com.sap.incture.IMO_PM.formatter.formatter.fnSetTaskNotifBtnVisible'}"/>
			</OverflowToolbar>
		</headerToolbar>
		<content>
			<VBox items="{oWorkOrderDetailModel>/HEADERTONOTIFNAV}">
				<items>
					<l:Grid class="sapUiTinyMarginTop">
						<l:content>
							<VBox>
								<Label text="{i18n>NOTIFICATION}"></Label>
								<Input value="{oWorkOrderDetailModel>NotifNo}" maxLength="10" enabled="false" tooltip="{oWorkOrderDetailModel>Qmnum}" class="inputRadius"></Input>
								<layoutData>
									<l:GridData span="XL2 L2 M4 S6"/>
								</layoutData>
							</VBox>
							<VBox>
								<Label text="{i18n>START_DATE}"></Label>
								<DatePicker minDate="" placeholder="dd MM yyyy" dateValue="{oWorkOrderDetailModel>Desstdate}" displayFormat="dd-MM-yyyy" enabled="false"
