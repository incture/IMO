							<customData>
								<core:CustomData key="dateType" value="PlanEndDate"/>
							</customData>
						</DatePicker>
						<layoutData>
							<l:GridData span="XL2 L2 M4 S6"/>
						</layoutData>
					</VBox>
					<VBox class="sapUiTinyMarginTop">
