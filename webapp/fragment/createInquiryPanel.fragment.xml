<core:FragmentDefinition xmlns="sap.m" xmlns:u="sap.ui.unified" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout"
	xmlns:cd="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Dialog showHeader="false" contentWidth="100%" class="iopwbDetailDialogClass iopwbInquiryDetailDialogClass"
		busy="{dashBoardModel>/busyIndicators/taskPanelBusy}" busyIndicatorDelay="0" beforeOpen="onTaskPanelBeforeOpen"
		afterOpen="onTaskPanelAfterOpen" cd:collabTaskType="Inquiry" verticalScrolling="false">
		<HBox>
			<VBox width="75%">
				<core:Fragment fragmentName="com.sap.incture.Incture_IOP.fragment.inquiryDetailsPanel" type="XML" id="idInquiryDetailsPanel"></core:Fragment>
			</VBox>
			<VBox width="25%" class="iopwbSplitBorderClass">
				<HBox justifyContent="End" height="0rem">
					<Button icon="sap-icon://sys-cancel" class="iopwbCloseBtnClass" type="Transparent" press="onCreateInquiryClose"/>
				</HBox>
				<core:Fragment fragmentName="com.sap.incture.Incture_IOP.fragment.taskDetailRightPanel" type="XML" id="idInquiryDetailRightPanel"></core:Fragment>
			</VBox>
		</HBox>
		<buttons>
			<Button text="Create Inquiry" class="sapUiSizeCompact iopBtnClass iopwbFooterBtnClass" press="onPressInquiryButtons"
				visible="{= ${oTaskPanelDetailModel>/taskEventDto/status} === 'NEW' &amp;&amp; ${dashBoardModel>/isWebReadOnlyRole} === false}"/>
			<Button text="Resolve" class="sapUiSizeCompact iopBtnClass iopwbFooterBtnClass" press="onPressInquiryButtons"
				visible='{parts:["dashBoardModel>/userData/isROC","dashBoardModel>/userData/isPOT","oTaskPanelDetailModel>/taskEventDto/owners","oTaskPanelDetailModel>/taskEventDto/status","dashBoardModel>/userData/userId", "oTaskPanelDetailModel>/taskEventDto/hasDispatchTask", "oTaskPanelDetailModel>/taskEventDto/origin", "oTaskPanelDetailModel>/taskEventDto/parentOrigin", "oTaskPanelDetailModel>/taskEventDto/group", "oTaskPanelDetailModel>/taskEventDto/editable", "dashBoardModel>/isWebReadOnlyRole"], formatter:"com.sap.incture.Incture_IOP.util.formatter.showResolveForwardFromInquiryBtn"}'/>
			<Button text="Update Inquiry" class="sapUiSizeCompact iopBtnClass iopwbFooterBtnClass" press="onPressInquiryButtons"
				visible='{parts:["oTaskPanelDetailModel>/taskEventDto/status","oTaskPanelDetailModel>/taskEventDto/group","dashBoardModel>/userData/sGroup","dashBoardModel>/isWebReadOnlyRole"], formatter:"com.sap.incture.Incture_IOP.util.formatter.showUpdateFromInquiryBtn"}'/>
			<Button text="Close Inquiry" class="sapUiSizeCompact iopBtnClass iopwbFooterBtnClass" press="onPressInquiryButtons"
				visible='{parts:["oTaskPanelDetailModel>/taskEventDto/status","oTaskPanelDetailModel>/taskEventDto/group", "oTaskPanelDetailModel>/taskEventDto/hasDispatchTask","dashBoardModel>/userData/sGroup", "dashBoardModel>/isWebReadOnlyRole"], formatter:"com.sap.incture.Incture_IOP.util.formatter.showCloseFromInquiryBtn"}'/>
			<Button text="Forward" class="sapUiSizeCompact iopBtnClass iopwbFooterBtnClass" press="onPressInquiryButtons"
				visible='{parts:["dashBoardModel>/userData/isROC","dashBoardModel>/userData/isPOT","oTaskPanelDetailModel>/taskEventDto/owners","oTaskPanelDetailModel>/taskEventDto/status","dashBoardModel>/userData/userId", "oTaskPanelDetailModel>/taskEventDto/hasDispatchTask", "oTaskPanelDetailModel>/taskEventDto/origin", "oTaskPanelDetailModel>/taskEventDto/parentOrigin", "oTaskPanelDetailModel>/taskEventDto/group", "oTaskPanelDetailModel>/taskEventDto/editable", "dashBoardModel>/isWebReadOnlyRole"], formatter:"com.sap.incture.Incture_IOP.util.formatter.showResolveForwardFromInquiryBtn"}'/>
			<Button text="Investigate" class="sapUiSizeCompact iopBtnClass iopwbFooterBtnClass" press="onPressInquiryButtons"
				visible='{parts:["dashBoardModel>/userData/isPOT","oTaskPanelDetailModel>/taskEventDto/owners","oTaskPanelDetailModel>/taskEventDto/status","dashBoardModel>/userData/userId","oTaskPanelDetailModel>/taskEventDto/hasDispatchTask", "oTaskPanelDetailModel>/taskEventDto/origin", "oTaskPanelDetailModel>/taskEventDto/parentOrigin", "oTaskPanelDetailModel>/taskEventDto/group", "oTaskPanelDetailModel>/taskEventDto/editable", "dashBoardModel>/isWebReadOnlyRole"], formatter:"com.sap.incture.Incture_IOP.util.formatter.showInvestigationFromInquiryBtn"}'/>
			<Button text="Dispatch" class="sapUiSizeCompact iopBtnClass iopwbFooterBtnClass" press="onPressInquiryButtons"
				visible='{parts:["dashBoardModel>/userData/isROC","oTaskPanelDetailModel>/taskEventDto/owners","oTaskPanelDetailModel>/taskEventDto/status","dashBoardModel>/userData/userId", "oTaskPanelDetailModel>/taskEventDto/hasDispatchTask", "oTaskPanelDetailModel>/taskEventDto/origin", "oTaskPanelDetailModel>/taskEventDto/parentOrigin", "oTaskPanelDetailModel>/taskEventDto/group", "oTaskPanelDetailModel>/taskEventDto/editable", "dashBoardModel>/isWebReadOnlyRole"], formatter:"com.sap.incture.Incture_IOP.util.formatter.showDispatchFromInquiryBtn"}'/>
			<Button text="Cancel" class="sapUiSizeCompact iopBtnClass iopwbFooterBtnClass" press="onPressInquiryButtons"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>