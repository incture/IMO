<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:ui="sap.ui.table">
	<Dialog title="{mLookupModel>/kpiWOHeader}" class="sapUiSizeCompact dialogHeader" contentWidth="50%">
		<content>
			<ui:Table rows="{mLookupModel>/kpiWOlist}" selectionMode="Single" selectionBehavior="RowOnly" rowSelectionChange="getSelectedKPIsWOs" visibleRowCount="8"
				class="sapUiSmallMargin tableCellNoBorder TableHeaderWhiteBG" enableSelectAll="false" id="WO_KPI_LIST_TBL">
				<ui:columns>
					<ui:Column hAlign="Begin" filterProperty="Number" sortProperty="Number" defaultFilterOperator="Contains">
						<Label text="{i18n>orderID}" design="Bold"/>
						<ui:template>
							<Text text="{mLookupModel>Number}"/>
						</ui:template>
					</ui:Column>
					<ui:Column hAlign="Begin" filterProperty="Description" sortProperty="Description" defaultFilterOperator="Contains">
						<Label text="{i18n>ORDER_DESC}" design="Bold"/>
						<ui:template>
							<Text text="{mLookupModel>Description}"/>
						</ui:template>
					</ui:Column>
				</ui:columns>
			</ui:Table>
		</content>
		<buttons>
			<Button text="{i18n>CANCEL}" press="closeKPIWOListPopover" class="transparentBtn createNotifClearBtn" icon="sap-icon://decline" iconFirst="true"></Button>
			<!--<Button text="{i18n>viewDetails}" press="viewKPIWODetails" class="murphyDialogBtn"></Button>-->
		</buttons>
	</Dialog>
</core:FragmentDefinition>