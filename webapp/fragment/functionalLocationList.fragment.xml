<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:layout="sap.ui.layout"
	xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" class="sapUiSizeCompact">
	<Dialog title="{i18n>Dialog_title_funLoc}" contentWidth="35%" verticalScrolling="false" class="dialogHeader sapUiSizeCompact">
		<content>
			<SearchField liveChange="onSearchFnLocs" width="90%" class="sapUiTinyMargin sapSearchFieldBorderClass"/>
			<ScrollContainer height="20rem" vertical="true" horizontal="false" focusable="false" class="sapUiTinyMargin">
				<Table sticky="ColumnHeaders" mode="SingleSelectLeft" selectionChange="onFnLocSelect" growing="false" id ="idFunLocListTable" items="{path: 'mLookupModel>/aFnLocsList'}" class="tableCellNoBorder TableHeaderWhiteBG">
					<columns>
						<Column>
							<Text text="{i18n>FUN_LOC}"/>
						</Column>
					</columns>
					<items>
						<ColumnListItem>
							<cells >
								<Text text="{mLookupModel>FuncLoc}"/>
							</cells>
						</ColumnListItem>
					</items>
				</Table>
				<Toolbar>
					<Link class="sapUiSmallMarginEnd" text="{i18n>LOAD_MORE}" press="handleLoadFnLocs"/>
				</Toolbar>
			</ScrollContainer>
		</content>
		<beginButton>
			<Button text="{i18n>CANCEL}" press="onCancelDialogFunLoc" class="transparentBtn createNotifClearBtn" icon="sap-icon://decline" iconFirst="true"/>
		</beginButton>
		<!--<endButton>-->
		<!--	<Button text="{i18n>SEARCH}" press="onSearchWOFilter" type="Emphasized"/>-->
		<!--</endButton>-->
	</Dialog>
</core:FragmentDefinition >