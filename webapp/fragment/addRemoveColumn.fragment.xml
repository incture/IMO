<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:layout="sap.ui.layout"
	xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" class="sapUiSizeCompact">
	<Dialog title="Add or Remove Column" contentWidth="30%" class="sapUiSizeCompact dialogHeader">
		<!--<Page title="Add or Remove Column" showHeader="true" class="whiteBG">-->
		<content>
			<!--<VBox>-->
			<layout:Grid defaultSpan="XL6 L6 M6 S12" class="sapUiTinyMargin">
				<CheckBox text="Functional Location" selected="{mLookupModel>/sTFunctLoc}"/>
				<CheckBox text="Main Work Center" selected="{mLookupModel>/sTmainWrkCtr}"/>
				<CheckBox text="WorkOrder Type" selected="{mLookupModel>/sTWoType}"/>
				<CheckBox text="Tech Id" selected="{mLookupModel>/sTtechId}"/>
				<CheckBox text="Equipment Description" selected="{mLookupModel>/sTequipDesc}"/>
				<CheckBox text="Assigned Tech" selected="{mLookupModel>/sTassignedTech}"/>
				<CheckBox text="System Status" selected="{mLookupModel>/sTsysStatus}"/>
				<CheckBox text="User Status" selected="{mLookupModel>/sTuserStatus}"/>
				<CheckBox text="Plant" selected="{mLookupModel>/sTmaintPlant}"/>
				<CheckBox text="BD Flag" selected="{mLookupModel>/sTbdFlag}"/>
				<CheckBox text="Priority" selected="{mLookupModel>/sTpriority}"/>
				<CheckBox text="Created By" selected="{mLookupModel>/sTcreatedBy}"/>
				<CheckBox text="Created Date" selected="{mLookupModel>/sTCreatedDate}"/>
			</layout:Grid>
			<!--</VBox>-->
		</content>
		<!--	<footer>
				<Toolbar>-->
		<buttons>
			<Button text="Select All" press="onSelectAll" type="Transparent" icon="sap-icon://accept" iconFirst="true"/>
			<Button text="Reset" press="setTableVisibile" type="Transparent" icon="sap-icon://reset" iconFirst="true"/>
			<!--<ToolbarSpacer/>-->
			<Button text="Close" press="onCloseColumnAdd" class="transparentBtn createNotifClearBtn" icon="sap-icon://decline" iconFirst="true"/>
		</buttons>
		<!--</Toolbar>
			</footer>
		</Page>-->
	</Dialog>
</core:FragmentDefinition >