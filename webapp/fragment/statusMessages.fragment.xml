<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:ui="sap.ui.table">
	<Dialog title="{i18n>SUCCESS_ERROR}" class="sapUiSizeCompact dialogHeader" contentWidth="50%">
		<content>
			<ui:Table rows="{mLookupModel>/messages}" selectionMode="None" visibleRowCount="4" class="sapUiSmallMargin tableCellNoBorder TableHeaderWhiteBG" enableSelectAll="false">
				<ui:columns>
					<ui:Column hAlign="Center" width="10rem">
						<Label text="{i18n>STATUS}"/>
						<ui:template>
							<!--<Text text="{mLookupModel>Status}"/>-->
							<HBox justifyContent="Center">
								<core:Icon src="sap-icon://circle-task-2" class="successCircle" visible="{= ${mLookupModel>Status} === 'S'}" tooltip="Success"/>
								<core:Icon src="sap-icon://circle-task-2" class="errorCircle" visible="{= ${mLookupModel>Status} !== 'S'}" tooltip="Error"/>
							</HBox>
						</ui:template>
					</ui:Column>
					<ui:Column hAlign="Center">
						<Label text="{i18n>MESSAGE}"/>
						<ui:template>
							<Text text="{mLookupModel>Message}"/>
						</ui:template>
					</ui:Column>
				</ui:columns>
			</ui:Table>
		</content>
		<buttons>
			<Button text="{i18n>OK}" press="closeSuccesErrPopup" type="Emphasized"></Button>
		</buttons>
	</Dialog>
</core:FragmentDefinition>